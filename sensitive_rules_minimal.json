{
  "description": "GogoSpider 最小规则集 - 只检测10个最高危的敏感信息",
  "version": "3.1",
  "use_case": "快速扫描、性能优先场景",
  "rules": {
    "_comment": "========== 只保留最高危的10个规则 ==========",
    
    "AWS S3 Access Key": {
      "pattern": "(A3T[A-Z0-9]|AKIA|AGPA|AIDA|AROA|AIPA|ANPA|ANVA|ASIA)[A-Z0-9]{16}",
      "severity": "HIGH",
      "mask": true,
      "description": "AWS Access Key ID - 可能泄露S3存储桶访问权限"
    },
    
    "阿里云OSS AccessKey": {
      "pattern": "(?i)(aliyun|oss)[_-]?access[_-]?key[_-]?(id|ID)['\"]?\\s*[:=]\\s*['\"]?(LTAI[A-Za-z0-9]{12,20}|[A-Z0-9]{16,24})",
      "severity": "HIGH",
      "mask": true,
      "description": "阿里云OSS AccessKey ID - 存储桶访问凭证"
    },
    
    "腾讯云COS SecretId": {
      "pattern": "(?i)(tencent|qcloud|cos)[_-]?secret[_-]?(id|ID)['\"]?\\s*[:=]\\s*['\"]?(AKID[A-Za-z0-9]{32,36}|[A-Za-z0-9]{32,40})",
      "severity": "HIGH",
      "mask": true,
      "description": "腾讯云COS SecretId - 存储桶访问凭证"
    },
    
    "SSH Private Key": {
      "pattern": "-----BEGIN ((EC|PGP|DSA|RSA|OPENSSH) )?PRIVATE KEY-----",
      "severity": "HIGH",
      "mask": true,
      "description": "SSH私钥"
    },
    
    "数据库密码": {
      "pattern": "(?i)(db|database|mysql|postgres|mongo)[_-]?(password|passwd|pwd)['\"]?\\s*[:=]\\s*['\"]([^'\"\\s]{6,})",
      "severity": "HIGH",
      "mask": true,
      "description": "数据库密码"
    },
    
    "MySQL Connection": {
      "pattern": "(?i)(mysql|mariadb)://[^:]+:[^@]+@[^/\\s]+",
      "severity": "HIGH",
      "mask": true,
      "description": "MySQL连接字符串（含密码）"
    },
    
    "微信AppSecret": {
      "pattern": "(?i)(wechat|weixin|wx)[_-]?app[_-]?secret['\"]?\\s*[:=]\\s*['\"]?([a-z0-9]{32})",
      "severity": "HIGH",
      "mask": true,
      "description": "微信AppSecret - 严重泄露风险"
    },
    
    "支付宝私钥": {
      "pattern": "(?i)alipay[_-]?private[_-]?key['\"]?\\s*[:=]\\s*['\"]?(MII[A-Za-z0-9+/=]{100,})",
      "severity": "HIGH",
      "mask": true,
      "description": "支付宝应用私钥 - 严重泄露风险"
    },
    
    "管理员密码": {
      "pattern": "(?i)(admin|root)[_-]?(password|passwd|pwd)['\"]?\\s*[:=]\\s*['\"]([^'\"\\s]{6,})",
      "severity": "HIGH",
      "mask": true,
      "description": "管理员密码"
    },
    
    "中国身份证": {
      "pattern": "[1-9]\\d{5}(18|19|20)\\d{2}((0[1-9])|(1[0-2]))(([0-2][1-9])|10|20|30|31)\\d{3}[0-9Xx]",
      "severity": "HIGH",
      "mask": true,
      "description": "中国身份证号"
    }
  }
}

