{
  "target_url": "https://example.com",
  
  "filter_settings": {
    "comment": "URL过滤管理器配置（v1.0新架构）",
    
    "preset": "balanced",
    "mode": "balanced",
    "enabled": true,
    
    "performance": {
      "enable_caching": true,
      "cache_size": 10000,
      "enable_early_stop": true
    },
    
    "debug": {
      "enable_trace": false,
      "trace_buffer_size": 100,
      "verbose_logging": false
    },
    
    "scope": {
      "allow_subdomains": true,
      "allow_http": true,
      "allow_https": true,
      "external_link_action": "degrade"
    },
    
    "type_classifier": {
      "static_resource_action": "degrade",
      "js_file_action": "allow",
      "css_file_action": "degrade"
    },
    
    "business_value": {
      "min_score": 30.0,
      "high_value_threshold": 70.0,
      "enabled": true
    }
  },
  
  "depth_settings": {
    "max_depth": 3
  },
  
  "strategy_settings": {
    "enable_static_crawler": true,
    "enable_dynamic_crawler": true
  }
}

