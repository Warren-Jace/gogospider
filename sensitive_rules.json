      "mask": true,
      "description": "[REx] APIs - Github App Token"
    },
    "Github OAuth Access Token": {
      "pattern": "gho_[0-9a-zA-Z]{36}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Github OAuth Access Token"
    },
    "Github Personal Access Token": {
      "pattern": "ghp_[0-9a-zA-Z]{36}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Github Personal Access Token"
    },
    "Github Refresh Token": {
      "pattern": "ghr_[0-9a-zA-Z]{76}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Github Refresh Token"
    },
    "Gitlab Personal Access Token": {
      "pattern": "glpat-[0-9a-zA-Z\\-]{20}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Gitlab Personal Access Token"
    },
    "Google (GCP) Service-account": {
      "pattern": "\"type.+:.+\"service_account",
      "severity": "MEDIUM",
      "mask": true,
      "description": "[REx] APIs - Google (GCP) Service-account"
    },
    "Google Cloud API Key": {
      "pattern": "AIza[0-9A-Za-z\\-_]{35}",
      "severity": "HIGH",
      "mask": true,
      "description": "Google Cloud API Key"
    },
    "Google Drive Oauth": {
      "pattern": "[0-9]+-[0-9A-Za-z_]{32}\\.apps\\.googleusercontent\\.com",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Google Drive Oauth"
    },
    "Google Oauth Access Token": {
      "pattern": "ya29\\.[0-9A-Za-z_\\-]+",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Google Oauth Access Token"
    },
    "Grafana API Key": {
      "pattern": "(?i)eyJrIjoi[a-z0-9_=\\-]{72,92}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Grafana API Key"
    },
    "Grafana cloud api token": {
      "pattern": "glc_[A-Za-z0-9\\+/]{32,}={0,2}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Grafana cloud api token"
    },
    "Grafana service account token": {
