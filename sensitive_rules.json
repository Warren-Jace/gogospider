{
  "description": "GogoSpider 增强规则集 - 集成 RExpository 高价值规则",
  "version": "4.0",
  "rules": {
    "AWS AppSync GraphQL Key": {
      "pattern": "da2-[a-z0-9]{26}",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - AWS AppSync GraphQL Key"
    },
    "AWS MWS Key": {
      "pattern": "amzn\\.mws\\.[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - AWS MWS Key"
    },
    "AWS S3 Access Key": {
      "pattern": "(A3T[A-Z0-9]|AKIA|AGPA|AIDA|AROA|AIPA|ANPA|ANVA|ASIA)[A-Z0-9]{16}",
      "severity": "HIGH",
      "mask": true,
      "description": "AWS Access Key ID - 可能泄露S3存储桶访问权限"
    },
    "AWS S3 Bucket": {
      "pattern": "[a-z0-9][a-z0-9.-]{1,61}[a-z0-9]\\.s3[.-][a-z0-9-]*\\.amazonaws\\.com",
      "severity": "MEDIUM",
      "mask": false,
      "description": "AWS S3存储桶域名"
    },
    "AWS S3 Secret Key": {
      "pattern": "(?i)(aws_secret|aws.{0,20}secret)['\"]?\\s*[:=]\\s*['\"]?([0-9a-zA-Z/+]{40})",
      "severity": "HIGH",
      "mask": true,
      "description": "AWS Secret Access Key - 严重泄露风险"
    },
    "Age Secret Key": {
      "pattern": "AGE-SECRET-KEY-1[QPZRY9X8GF2TVDW0S3JN54KHCE6MUA7L]{58}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Age Secret Key"
    },
    "Airtable API Key": {
      "pattern": "[\"']?air[-_]?table[-_]?api[-_]?key[\"']?[=:][\"']?.+[\"']\"\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Airtable API Key"
    },
    "Alibaba Access Key ID": {
      "pattern": "(?i)(LTAI)[a-z0-9]{20}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Alibaba Access Key ID"
    },
    "Alibaba Secret Key": {
      "pattern": "(?i)(alibaba[a-z0-9_ \\.,\\-]{0,25})(=|\u003e|:=|\\|\\|:|\u003c=|=\u003e|:).{0,5}['\"]([a-z0-9]{30})['\"]\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Alibaba Secret Key"
    },
    "Asana Client ID": {
      "pattern": "(?i)((asana[a-z0-9_ \\.,\\-]{0,25})(=|\u003e|:=|\\|\\|:|\u003c=|=\u003e|:).{0,5}['\"]([0-9]{16})['\"])|((asana[a-z0-9_ \\.,\\-]{0,25})(=|\u003e|:=|\\|\\|:|\u003c=|=\u003e|:).{0,5}['\"]([a-z0-9]{32})['\"])\n",
      "severity": "LOW",
      "mask": false,
      "description": "[REx] APIs - Asana Client ID"
    },
    "Atlassian API Key": {
      "pattern": "(?i)(atlassian[a-z0-9_ \\.,\\-]{0,25})(=|\u003e|:=|\\|\\|:|\u003c=|=\u003e|:).{0,5}['\"]([a-z0-9]{24})['\"]\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Atlassian API Key"
    },
    "Authorization Token": {
      "pattern": "(?i)(authorization|auth[_-]?token)['\"]?\\s*[:=]\\s*['\"]?([a-zA-Z0-9_\\-\\.]{20,})",
      "severity": "MEDIUM",
      "mask": true,
      "description": "Authorization Token"
    },
    "Basic Auth Credentials": {
      "pattern": "://[a-zA-Z0-9]+:[a-zA-Z0-9]+@[a-zA-Z0-9]+\\.[a-zA-Z]+",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Basic Auth Credentials"
    },
    "Clojars API Key": {
      "pattern": "(CLOJARS_)[a-zA-Z0-9]{60}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Clojars API Key"
    },
    "Cloudinary Basic Auth": {
      "pattern": "cloudinary://[0-9]{15}:[0-9A-Za-z]+@[a-z]+",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Cloudinary Basic Auth"
    },
    "Databricks API Key": {
      "pattern": "dapi[a-h0-9]{32}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Databricks API Key"
    },
    "Discord API Key, Client ID \u0026 Client Secret": {
      "pattern": "(?i)((discord[a-z0-9_ \\.,\\-]{0,25})(=|\u003e|:=|\\|\\|:|\u003c=|=\u003e|:).{0,5}['\"]([a-h0-9]{64}|[0-9]{18}|[a-z0-9=_\\-]{32})['\"])\n",
      "severity": "HIGH",
      "mask": false,
      "description": "[REx] APIs - Discord API Key, Client ID \u0026 Client Secret"
    },
    "Doppler API Key": {
      "pattern": "(dp\\.pt\\.)[a-zA-Z0-9]{43}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Doppler API Key"
    },
    "Dropbox API Key": {
      "pattern": "sl.[a-zA-Z0-9_-]{136}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Dropbox API Key"
    },
    "Duffel API Key": {
      "pattern": "duffel_(test|live)_[a-zA-Z0-9_-]{43}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Duffel API Key"
    },
    "Dynatrace API Key": {
      "pattern": "dt0c01\\.[a-zA-Z0-9]{24}\\.[a-z0-9]{64}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Dynatrace API Key"
    },
    "EasyPost API Key": {
      "pattern": "EZAK[a-zA-Z0-9]{54}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - EasyPost API Key"
    },
    "EasyPost test API Key": {
      "pattern": "EZTK[a-zA-Z0-9]{54}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - EasyPost test API Key"
    },
    "Facebook Access Token": {
      "pattern": "EAACEdEose0cBA[0-9A-Za-z]+",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Facebook Access Token"
    },
    "Facebook Client ID": {
      "pattern": "([fF][aA][cC][eE][bB][oO][oO][kK]|[fF][bB])(.{0,20})?['\"][0-9]{13,17}",
      "severity": "LOW",
      "mask": false,
      "description": "[REx] APIs - Facebook Client ID"
    },
    "Facebook Oauth": {
      "pattern": "[fF][aA][cC][eE][bB][oO][oO][kK].*['|\"][0-9a-f]{32}['|\"]\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Facebook Oauth"
    },
    "Facebook Secret Key": {
      "pattern": "([fF][aA][cC][eE][bB][oO][oO][kK]|[fF][bB])(.{0,20})?['\"][0-9a-f]{32}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Facebook Secret Key"
    },
    "Frame.io API Key": {
      "pattern": "fio-u-[a-zA-Z0-9_=\\-]{64}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Frame.io API Key"
    },
    "Generic API Key": {
      "pattern": "(?i)(api[_-]?key|apikey|api[_-]?secret)['\"]?\\s*[:=]\\s*['\"]?([a-zA-Z0-9_\\-]{20,})",
      "severity": "HIGH",
      "mask": true,
      "description": "通用API Key"
    },
    "GitHub Fine-Grained Personal Access Token": {
