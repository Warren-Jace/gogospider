    },
    "Heroku API Key": {
      "pattern": "[hH][eE][rR][oO][kK][uU].{0,30}[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Heroku API Key"
    },
    "JWT Token": {
      "pattern": "eyJ[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_=]+\\.?[A-Za-z0-9-_.+/=]*",
      "severity": "MEDIUM",
      "mask": true,
      "description": "JWT Token"
    },
    "Linear API Key": {
      "pattern": "(lin_api_[a-zA-Z0-9]{40})\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Linear API Key"
    },
    "LinkedIn Client ID": {
      "pattern": "linkedin(.{0,20})?['\"][0-9a-z]{12}['\"]\n",
      "severity": "LOW",
      "mask": false,
      "description": "[REx] APIs - LinkedIn Client ID"
    },
    "LinkedIn Secret Key": {
      "pattern": "linkedin(.{0,20})?['\"][0-9a-z]{16}['\"]\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - LinkedIn Secret Key"
    },
    "Mailchimp API Key": {
      "pattern": "[0-9a-f]{32}-us[0-9]{1,2}",
      "severity": "HIGH",
