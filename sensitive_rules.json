    },
    "OpenAI API Token": {
      "pattern": "sk-[A-Za-z0-9]{48}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - OpenAI API Token"
    },
    "PayPal Braintree Access Token": {
      "pattern": "access_token\\$production\\$[0-9a-z]{16}\\$[0-9a-f]{32}\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - PayPal Braintree Access Token"
    },
    "Plaid API Token": {
      "pattern": "(access-(?:sandbox|development|production)-[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})\n",
      "severity": "HIGH",
      "mask": true,
      "description": "[REx] APIs - Plaid API Token"
    },
    "PlanetScale OAuth token": {
      "pattern": "(pscale_oauth_[a-zA-Z0-9_\\.\\-]{32,64})\n",
