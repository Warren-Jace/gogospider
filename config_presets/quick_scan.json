{
  "_comment": "快速扫描模式 - 适合快速了解目标网站结构",
  "_scenario": "适用场景：初步侦查、快速测试、时间紧急",
  
  "target_url": "",
  
  "blacklist_settings": {
    "enabled": true,
    "domains": ["*.gov.cn", "*.edu.cn", "*.mil.cn"],
    "strict_mode": true
  },
  
  "scope_settings": {
    "enabled": true,
    "exclude_extensions": ["jpg", "png", "gif", "css", "js", "woff", "mp4", "pdf", "zip"],
    "allow_subdomains": false,
    "stay_in_domain": true
  },
  
  "depth_settings": {
    "max_depth": 3,
    "max_pages": 200,
    "scheduling_algorithm": "BFS"
  },
  
  "strategy_settings": {
    "enable_static_crawler": true,
    "enable_dynamic_crawler": false,
    "enable_js_analysis": true,
    "enable_api_inference": true,
    "enable_common_path_scan": false
  },
  
  "anti_detection_settings": {
    "request_delay": 200000000,
    "timeout": 15,
    "retry_times": 2
  },
  
  "deduplication_settings": {
    "similarity_threshold": 0.90,
    "enable_dom_deduplication": true,
    "enable_smart_param_dedup": true,
    "max_param_value_variants_per_group": 2,
    "enable_business_aware_filter": true,
    "business_filter_max_low_value": 1,
    "business_filter_max_mid_value": 3,
    "business_filter_max_high_value": 10
  },
  
  "rate_limit_settings": {
    "enabled": false
  },
  
  "sensitive_detection_settings": {
    "enabled": true,
    "min_severity": "MEDIUM",
    "rules_file": "./sensitive_rules_config.json"
  },
  
  "output_settings": {
    "format": "json",
    "json_mode": "line",
    "verbose": false
  },
  
  "log_settings": {
    "level": "INFO",
    "show_metrics": true
  }
}

